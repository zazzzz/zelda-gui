{
  "private": true,
  "name": "zelda-gui",
  "productName": "Zelda-GUI",
  "version": "1.0.0",
  "description": "Zelda GUI",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "NODE_ENV=development electron .",
    "pack": "electron-packager . --icon=icon/zzz.icns --out=release --overwrite",
