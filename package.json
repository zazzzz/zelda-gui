    "electron-store": "^1.3.0",
    "zelda-cli": "^0.2.7"
  },
  "devDependencies": {
    "antd": "^2.13.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-dva-hmr": "^0.3.2",
    "babel-plugin-import": "^1.4.0",
    "babel-plugin-transform-runtime": "^6.23.0",
