    "dva": "^2.0.1",
    "electron": "^18.3.7",
    "electron-packager": "^9.0.1",
    "eslint": "^4.6.1",
    "eslint-config-alloy": "^1.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.3.0",
